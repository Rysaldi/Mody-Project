{
  "expo": {
    "name": "Mody",
    "slug": "Mody",
    "version": "1.0.0",
    "orientation": "portrait",
    "description": "Mody is your go to app for managing your cashflow to reach your financial dream. You can track your expenses, income, spending habits, and many more. With Mody, you can make sure that every single cent of your money is spent responsibly.",
    "icon": "./assets/moody_pertama.png",
    "privacy": "public",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/moody_pertama.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
